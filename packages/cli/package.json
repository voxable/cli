{
  "name": "@voxable/cli",
  "description": "A client for the Voxable conversation design platform's API.",
  "version": "0.1.6",
  "author": "Voxable, Inc. <howdy@voxable.io>",
  "bin": {
    "voxable": "./bin/run"
  },
  "bugs": "https://github.com/voxable/cli/issues",
  "dependencies": {
    "@oclif/plugin-plugins": "1.10.0",
    "@types/cli-table": "0.3.0",
    "@voxable/client": "^0.1.4",
    "@voxable/command": "^0.1.3",
    "chalk": "4.1.1",
    "cli-table": "0.3.6",
    "cli-ux": "5.5.1",
    "cosmiconfig": "7.0.0",
    "terminal-link": "3.0.0",
    "tslib": "^1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/voxable/cli",
  "keywords": [
    "nlu",
    "cui",
    "vui",
    "voice"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "voxable",
    "scope": "voxable",
    "plugins": [
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "voxable/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -f tsconfig.tsbuildinfo && rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "typescript": "4.2.4"
  },
  "gitHead": "de47b9104ac85cc16b82d206c20206a1d6a2bf80"
}
